[
  {
    "name": "Error",
    "pattern": "ERROR|ERR",
    "type": "error",
    "color": "#FF0000"
  },
  {
    "name": "Warning",
    "pattern": "WARNING|WARN|WRN",
    "type": "warning",
    "color": "#FFA500"
  },
  {
    "name": "AssertionFailed",
    "pattern": "\\[Assert FAILED\\]|AssertionError|assertion.*failed",
    "type": "assertion",
    "color": "#FF8800"
  },
  {
    "name": "ExceptionFailed",
    "pattern": "\\[Expect FAILED\\]|Exception|expect.*failed",
    "type": "exception",
    "color": "#FF8800"
  },
  {
    "name": "TestCase",
    "pattern": "\\.py::\\w+::\\w+",
    "type": "testcase",
    "color": "#00AA00"
  }
]