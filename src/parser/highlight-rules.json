[

  {
    "name": "Warning",
    "pattern": "\\b(WARNING|WARN|WRN)\\b|\\[WARNING\\]",
    "type": "warning",
    "color": "#FFA500"
  },
  
  {
    "name": "AssertionFailed",
    "pattern": "\\[Assert FAILED\\]|AssertionError|\\bassert.*failed\\b",
    "type": "assertion",
    "color": "#FF8800"
  },
  {
    "name": "ExceptionFailed",
    "pattern": "\\[Expect FAILED\\]|Exception|\\bexpect.*failed\\b",
    "type": "exception",
    "color": "#FF8800"
  },
  {
    "name": "Error",
    "pattern": "\\[ERR\\]|\\bERROR\\b|\\bError\\b",
    "type": "error",
    "color": "#FF0000"
  },
  {
    "name": "TestCaseFailed",
    "pattern": "\\b(FAILED|FAILURE)\\b.*\\.py::\\w+::\\w+",
    "type": "testcase-failed",
    "color": "#FF0000"
  },
  {
    "name": "TestCasePassed",
    "pattern": "\\b(PASSED|SUCCESS)\\b.*\\.py::\\w+::\\w+",
    "type": "testcase",
    "color": "#00AA00"
  }
]